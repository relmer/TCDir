{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug TCDir (x64)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\x64\\Debug\\TCDir.exe",
            "args": ["\\\\.\\unc\\relmer-blue"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Debug x64"
        },
        {
            "name": "Debug TCDir /s (x64)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\x64\\Debug\\TCDir.exe",
            "args": ["C:\\Windows\\System32", "/s"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Debug x64"
        },
        {
            "name": "Debug TCDir /w /s (x64)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\x64\\Debug\\TCDir.exe",
            "args": ["C:\\Windows\\System32", "/w", "/s"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Debug x64"
        },
        {
            "name": "Debug TCDir /env (x64)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\x64\\Debug\\TCDir.exe",
            "args": ["C:\\Windows\\System32", "/env"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [ { "name" : "TCDIR", "value" : "D=LightGreen;S=Yellow;.cpp=White on Bluepickles" } ],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Debug x64"
        },
        {
            "name": "Run Release TCDir (x64)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}\\x64\\Release\\TCDir.exe",
